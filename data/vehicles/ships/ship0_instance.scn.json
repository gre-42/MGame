[
  {
    "declare_macro": "create_ship0",
    "# content": {
      "playback": "generic_car.create",
      "let": {
        "name": "ship0",
        "tesuffix": "%suffix",
        "tedecimate": "",
        "wheels": "%%vehicles/ship0/wheels",
        "engine_audio": "%%vehicles/ship0/engine_audio"
      }
    },
    "content": {
      "call": "create_generic_car",
      "arguments": {
        "name": "ship0",
        "tesuffix": "%suffix",
        "tedecimate": "",
        "if_with_graphics": "%if_with_graphics",
        "if_with_physics": "%if_with_physics",
        "mute": "%mute",
        "velocity": "%velocity",
        "angular_velocity": "%angular_velocity",
        "hand_brake_pulled": "%hand_brake_pulled"
      }
    }
  },
  {
    "declare_macro": "create_player_car_internals_ship0",
    "content": {
      "playback": "generic_car.create_player_internals"
    }
  },
  {
    "declare_macro": "create_player_car_externals_ship0",
    "content": {
      "playback": "generic_car.create_player_externals",
      "let": {
        "camera_class": "ship",
        "cockpit_camera_position": [-0.4, 0.4, -0.3]
      }
    }
  }
]
