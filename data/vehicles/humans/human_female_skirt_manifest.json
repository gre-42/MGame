{
  "id": "female_skirt",
  "title": "Female skirt",
  "on_init": [
    { "include": "human_resource_box.scn.json" },
    { "include": "human_resource_mh1.scn.json" },
    { "include": "human_instance_mh1.scn.json" },
    {
      "declare_macro": "remote.vehicle.create_female_skirt",
      "content": { "playback": "create_female_skirt" }
    }
  ],
  "on_before_select": {
    "call": "globals",
    "arguments": {
      "selected_vehicle_id_$full_user_name": "female_skirt"
    }
  },
  "database": {
    "suffix": "female_skirt",
    "vehicle_class": "avatar",
    "node_prefixes": ["human_node"],
    "decimate": "_d2",
    "decimate1": "_d1",
    "spawn_y_offset": 0.7,
    "waypoint_dy": 0.7,
    "max_tire_angle": null,
    "wheel": "legs",
    "wheel_mount_0":  [0, 0, 0],
    "wheel_mount_1":  [0, 1, 0],
    "mass": 70,
    "com": [0, 0, 0],

    "angular_vels": [-4000, -2500, 0],
    "powers": ["nan", 2.7, 2.7],
    "gear_ratios": [-1, 1],
    "w_clutch": 800
  },
  "required": {
    "fixed": [],
    "dynamic": "(%selected_game_mode in {'team_deathmatch'})"
  }
}
