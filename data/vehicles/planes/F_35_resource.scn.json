      "call": "obj_resource",
      "arguments": {
        "name": "%name",
        "filename": "models/sketchfab.com/f-35c-lightning-ii/$subdir/$name.obj",
        "config": {
          "position": [0, -3, 0],
          "rotation": [0, 180, 0],
          "scale": [10, 10, 10],
          "center_distances": ["%dist_min", "%dist_max"],
