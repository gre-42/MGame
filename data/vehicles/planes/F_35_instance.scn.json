          "exclusive_node": null,
          "left": "opponent_zoom_left",
          "right": "opponent_zoom_right",
          "bottom": "opponent_zoom_bottom",
          "top": "opponent_zoom_top",
          "fov": 30,
          "zoom": 1.5,
          "background_color": ["nan", "nan", "nan"]
        }
      },
      {
        "required": ["%behavior != 'none'"],
