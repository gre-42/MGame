      "extrude_curb_amount": 0,
      "extrude_street_amount": -0.05,
      "extrude_grass_amount": 0.05,
      "extrude_elevated_grass_amount": 0.5,
      "extrude_water_floor_amout": -2,
      "building_grass_width": 0,
