{
  "declare_macro": "osm_resource_forest0",
  "content": {
    "call": "osm_resource",
    "literals": {
      "name": "osm_map",
      "filename": "$__DIR__/map_forest0.osm",
      "cache_filename": "$__APPDATA__/forest0.cereal.binary",
      "heightmap": "$__DIR__/heightmap.pgm",
      "terrain_undefined_textures": ["textures/debug/pink.png"],
      "terrain_grass_textures": [
        "#Ground037_1",
        "#Ground037_8"],
      "bounding_terrain_type": "grass",
      "default_terrain_type": "grass",
      "terrain_undefined_materials": "surface_base_stone",
      "street_materials": "surface_base_gravel",
      "grass_dirt_texture": "#Ground025",
      "street_dirt_texture": "#Asphalt016_Dirt",
      "street_crossing_texture": "#Road007_No_Lines",
      "street_textures": [
        {"lanes": 1, "textures": ["#Ground038_Way_Wheels"]}],
      "racing_line_texture": "#racing_line",
      "racing_line_track": "$__DIR__/racing_line_forest0.m",
      "racing_line_playback": "$__APPDATA__/$LEVEL_ID.checkpoints.m",
      "near_grass_resource_names": ["grass_tbdpec3r"],
      "dirt_decals_resource_names": [
        "Leaf018_2",
        "Leaf018_2_Red",
        "Paper(p:0.1)"],
      "default_street_width": 14,
      "wayside_resource_names": [
        {
          "min_dist": 3.5,
          "max_dist": 100,
          "resource_names": [
            "vegetation1.0(p:1)(hitbox:box_02_1_02_hitbox)",
            "vegetation1.1(p:1)(hitbox:box_02_1_02_hitbox)",
            "vegetation1.2(p:1)(hitbox:box_02_1_02_hitbox)",
            "vegetation1.3(p:1)(hitbox:box_02_1_02_hitbox)",
            "vegetation1.4(p:1)(hitbox:box_02_1_02_hitbox)",
            "vegetation1.5(p:1)(hitbox:box_02_1_02_hitbox)"]},
        {
          "min_dist": 1.5,
          "max_dist": 3,
          "resource_names": [
            "Stone_model_tex_small(p:0.2)(hitbox:box_02_1_02_hitbox)",
            "desert_rock01_small(p:0.2)(hitbox:box_02_1_02_hitbox)"]}],
      "scale": 1e-3,
      "uv_scale_terrain": 3e-1,
      "uv_period_terrain": 8,
      "uv_scale_street": 1e-1,
      "uv_scale_facade": 2.5e-1,
      "uv_scale_ceiling": 1e-1,
      "uv_scale_barrier_wall": 1e0,
      "uv_scale_highway_wall": 3e-1,
      "with_roofs": true,
      "with_ceilings": true,
      "building_bottom": 0,
      "default_building_top": 10,
      "default_barrier_top": 2,
      "remove_backfacing_triangles": true,
      "with_tree_nodes": true,
      "forest_outline_tree_distance": "inf",
      "forest_outline_tree_inwards_distance": 1,
      "much_grass_distance": "inf",
      "much_near_grass_distance": 3,
      "raceway_beacon_distance": "inf",
      "with_terrain": true,
      "with_buildings": true,
      "only_raceways_and_walls": false,
      "highway_name_pattern": "",
      "excluded_highways": [
        "pedestrian",
        "path",
        "footway",
        "cycleway",
        "steps"],
      "path_tags": [],
      "smoothed_highways": ["raceway"],
      "max_smooth_highway_length": 4,
      "steiner_point_distances_road": [0, 2, 30, 100],
      "steiner_point_distances_steiner": ["inf", 7, 20, 100],
      "curb_alpha": 1,
      "curb2_alpha": 1,
      "curb_uv_x": "nan",
      "curb2_uv_x": "nan",
      "raise_streets_amount": 0,
      "extrude_curb_amount": 0,
      "extrude_street_amount": 0,
      "extrude_wall_amount": 0,
      "street_light_resource_names": [],
      "max_wall_width": 10,
      "with_height_bindings": true,
      "street_node_smoothness": 1.7,
      "street_node_smoothing_iterations": 500,
      "street_edge_smoothness": 0,
      "terrain_edge_smoothness": 0.1,
      "driving_direction": "right",
      "blend_street": true,
      "street_surface_central_resource_name": "road_ditch_central",
      "street_surface_endpoint0_resource_name": "road_ditch_endpoint_0",
      "street_surface_endpoint1_resource_name": "road_ditch_endpoint_1",
      "emissivity_factor": "%VANILLA_RALLY_EMISSIVITY_FACTOR",
      "ambience_factor": "%VANILLA_RALLY_AMBIENCE_FACTOR",
      "diffusivity_factor": "%VANILLA_RALLY_DIFFUSIVITY_FACTOR",
      "specularity_factor": "%VANILLA_RALLY_SPECULARITY_FACTOR"
    }
  }
}
