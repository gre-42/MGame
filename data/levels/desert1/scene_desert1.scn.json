        "CHECKPOINTS": null,
        "PACENOTES_FILE": "$__DIR__/pacenotes.json",
        "IF_PLAYBACK_CHECKPOINTS": false,
        "IF_PLAYBACK_TRACK_1": false,
        "IF_PLAYBACK_TRACK_2": false,
        "IF_PLAYBACK_TRACK_3": false,
        "IF_CHECKPOINTS": true,
        "IF_BURN_IN": true
      }
