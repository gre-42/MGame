      "IF_DIRTMAP": true,
      "IF_EXTRACT_ALIGNMENT_PLANES": false,
      "IF_GEN_GRIND_LINES": false,
      "IF_GEN_INSTANCES": false,
      "BARRIER_DEPTH": 0,
      "IF_WITH_GEOGRAPHIC_MAPPING": true,
      "DIRTMAP_TEXTURE": "#grf_2048",
