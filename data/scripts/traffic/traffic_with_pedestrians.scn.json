    {"playback": "traffic.create_npc_and_parking_car", "let": {"spawner_name": "pc_npc3", "asset_id": "van0",     "color": [0.74, 0.22, 0.24]}},
    {"playback": "traffic.create_npc_and_parking_car", "let": {"spawner_name": "pc_npc4", "asset_id": "sedan0",   "color": [0.95, 0.80, 0.25]}},
    {"playback": "traffic.create_npc_and_parking_car", "let": {"spawner_name": "pc_npc5", "asset_id": "sedan0",   "color": [0.74, 0.22, 0.24]}},
