        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "h_npc1",  "asset_id": "female_mh",    "color": [-1, -1, -1], "spawn_trigger": "bystanders"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "h_npc2",  "asset_id": "female_mh",    "color": [-1, -1, -1], "spawn_trigger": "bystanders"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "h_npc3",  "asset_id": "female_mh",    "color": [-1, -1, -1], "spawn_trigger": "bystanders"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "h_npc4",  "asset_id": "male_mh",      "color": [-1, -1, -1], "spawn_trigger": "bystanders"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "h_npc5",  "asset_id": "male_mh",      "color": [-1, -1, -1], "spawn_trigger": "bystanders"}},
