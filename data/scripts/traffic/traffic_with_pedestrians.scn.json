        {"playback": "traffic.create_npc_and_parking_car", "let": {"spawner_name": "pc_npc3", "asset_id": "sports_car0",  "color": [0.50, 0.50, 0.50]}},
        {"playback": "traffic.create_npc_and_parking_car", "let": {"spawner_name": "pc_npc4", "asset_id": "sedan0",       "color": [0.95, 0.80, 0.25]}},
