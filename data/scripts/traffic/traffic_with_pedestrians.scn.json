        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "h_npc7",  "asset_id": "female_skirt", "color": [-1, -1, -1], "spawn_trigger": "bystanders"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "h_npc8",  "asset_id": "female_skirt", "color": [-1, -1, -1], "spawn_trigger": "bystanders"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "h_npc9",  "asset_id": "female_skirt", "color": [-1, -1, -1], "spawn_trigger": "bystanders"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "h_npc10", "asset_id": "female_skirt", "color": [-1, -1, -1], "spawn_trigger": "bystanders"}},

        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "dh_npc1", "asset_id": "female_mh",    "color": [-1, -1, -1], "spawn_trigger": "none"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "dh_npc2", "asset_id": "female_mh",    "color": [-1, -1, -1], "spawn_trigger": "none"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "dh_npc3", "asset_id": "female_mh",    "color": [-1, -1, -1], "spawn_trigger": "none"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "dh_npc4", "asset_id": "male_mh",      "color": [-1, -1, -1], "spawn_trigger": "none"}},
        {"playback": "traffic.create_npc_and_human", "let": {"spawner_name": "dh_npc5", "asset_id": "male_mh",      "color": [-1, -1, -1], "spawn_trigger": "none"}},
