{
  "declare_macro": "create_plane_key_bindings",
  "content": [
    {
      "required": "%if_manual_drive",
      "call": "car_controller_idle_binding",
      "arguments": {
        "player": "%player_name",
        "node": "car_node$suffix"
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "car_controller_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.steer_left",
        "seat": "%externals_seat",
        "player": "%player_name",
        "node": "car_node$suffix",
        "steer_left_amount": 1
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "car_controller_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.steer_right",
        "seat": "%externals_seat",
        "player": "%player_name",
        "node": "car_node$suffix",
        "steer_left_amount": -1
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "plane_controller_idle_binding",
      "arguments": {
        "player": "%player_name",
        "node": "car_node$suffix"
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "plane_controller_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.roll_left",
        "seat": "%externals_seat",
        "player": "%player_name",
        "node": "car_node$suffix",
        "roll": -5
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "plane_controller_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.roll_right",
        "seat": "%externals_seat",
        "player": "%player_name",
        "node": "car_node$suffix",
        "roll": 5
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "plane_controller_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.pitch_up",
        "seat": "%externals_seat",
        "player": "%player_name",
        "node": "car_node$suffix",
        "pitch": 10
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "plane_controller_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.pitch_down",
        "seat": "%externals_seat",
        "player": "%player_name",
        "node": "car_node$suffix",
        "pitch": -10
      }
    },
    {
      "comment": {
        "call": "abs_key_binding",
        "arguments": {
          "user_id": "%user_id",
          "node": "car_node$suffix",
          "key": "LEFT_SHIFT",
          "gamepad_button": "B",
          "force": [0, 20000, 0]
        }
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "plane_controller_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.forward",
        "seat": "%externals_seat",
        "player": "%player_name",
        "node": "car_node$suffix",
        "turbine_power": "inf"
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "plane_controller_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.brake",
        "seat": "%externals_seat",
        "player": "%player_name",
        "node": "car_node$suffix",
        "brake": 20
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "plane_controller_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.yaw_left",
        "seat": "%externals_seat",
        "player": "%player_name",
        "node": "car_node$suffix",
        "yaw": -20
      }
    },
    {
      "required": "%if_manual_drive",
      "call": "plane_controller_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.yaw_right",
        "seat": "%externals_seat",
        "player": "%player_name",
        "node": "car_node$suffix",
        "yaw": 20
      }
    },
    {
      "call": "weapon_cycle_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.next_weapon",
        "seat": "%externals_seat",
        "player": "%player_name",
        "weapon_increment": 1
      }
    },
    {
      "call": "weapon_cycle_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.previous_weapon",
        "seat": "%externals_seat",
        "player": "%player_name",
        "weapon_increment": -1
      }
    },
    {
      "required": "%if_has_gun",
      "call": "gun_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "any.shoot",
        "seat": "%externals_seat",
        "player": "%player_name"
      }
    },
    {
      "required": "%if_has_gun",
      "call": "player_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "any.select_next_opponent",
        "seat": "%externals_seat",
        "player": "%player_name",
        "select_next_opponent": true
      }
    },
    {
      "call": "player_key_binding",
      "arguments": {
        "user_id": "%user_id",
        "id": "plane.select_next_vehicle",
        "seat": "%externals_seat",
        "player": "%player_name",
        "select_next_vehicle": true
      }
    }
  ]
}
