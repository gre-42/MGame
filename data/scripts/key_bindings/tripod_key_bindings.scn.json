{
  "declare_macro": "create_tripod_key_bindings",
  "content": [
    {
      "call": "create_print_camera_node_info_key_binding",
      "arguments": {
        "id": "print_camera_node_info",
        "role": "default"
      }
    },
    {
      "call": "rel_key_binding_tripod",
      "arguments": {
        "id": "tripod.move_left",
        "role": "default",
        "translation": [-1, 0, 0],
        "velocity_press": 5,
        "velocity_repeat": 400,
        "speed_cursor": 0.05
      }
    },
    {
      "call": "rel_key_binding_tripod",
      "arguments": {
        "id": "tripod.move_right",
        "role": "default",
        "translation": [1, 0, 0],
        "velocity_press": 5,
        "velocity_repeat": 400,
        "speed_cursor": 0.05
      }
    },
    {
      "call": "rel_key_binding_tripod",
      "arguments": {
        "id": "tripod.move_up",
        "role": "default",
        "translation": [0, 1, 0],
        "velocity_press": 5,
        "velocity_repeat": 400,
        "speed_cursor": 0.05
      }
    },
    {
      "call": "rel_key_binding_tripod",
      "arguments": {
        "id": "tripod.move_down",
        "role": "default",
        "translation": [0, -1, 0],
        "velocity_press": 5,
        "velocity_repeat": 400,
        "speed_cursor": 0.05
      }
    },
    {
      "call": "rel_key_binding_tripod",
      "arguments": {
        "id": "tripod.move_forward",
        "role": "default",
        "translation": [0, 0, -1],
        "velocity_press": 5,
        "velocity_repeat": 400,
        "speed_cursor": 0.05
      }
    },
    {
      "call": "rel_key_binding_tripod",
      "arguments": {
        "id": "tripod.move_backwards",
        "role": "default",
        "translation": [0, 0, 1],
        "velocity_press": 5,
        "velocity_repeat": 400,
        "speed_cursor": 0.05
      }
    },
    {
      "call": "rel_key_binding_tripod",
      "arguments": {
        "id": "avatar.rotate_left",
        "role": "default",
        "rotation_axis": [0, 1, 0],
        "angular_velocity_press": 0.1,
        "angular_velocity_repeat": 1,
        "speed_cursor": 0.05
      }
    },
    {
      "call": "rel_key_binding_tripod",
      "arguments": {
        "id": "avatar.rotate_right",
        "role": "default",
        "rotation_axis": [0, 1, 0],
        "angular_velocity_press": -0.1,
        "angular_velocity_repeat": -1,
        "speed_cursor": -0.05
      }
    },
    {
      "call": "rel_key_binding_tripod",
      "arguments": {
        "id": "avatar.rotate_up",
        "role": "default",
        "rotation_axis": [1, 0, 0],
        "angular_velocity_press": 0.1,
        "angular_velocity_repeat": 1,
        "speed_cursor": 0.05
      }
    },
    {
      "call": "rel_key_binding_tripod",
      "arguments": {
        "id": "avatar.rotate_down",
        "role": "default",
        "rotation_axis": [1, 0, 0],
        "angular_velocity_press": -0.1,
        "angular_velocity_repeat": -1,
        "speed_cursor": -0.05
      }
    }
  ]
}
