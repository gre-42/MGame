[
  {
    "declare_macro": "create_FN",
    "content": {
      "playback": "generic_car.create",
      "literals": {
        "NAME": "FN",
        "SUFFIX": "%SUFFIX",
        "IF_WITH_GRAPHICS": "%IF_WITH_GRAPHICS",
        "IF_WITH_PHYSICS": "%IF_WITH_PHYSICS",
        "DECIMATE": "%DECIMATE",
        "HAND_BRAKE_PULLED": "%HAND_BRAKE_PULLED",

        "SIZE": [1.7, 1.2, 4.1],
        "COM": [0, -0.2, -0.15],

        "MASS": 2000,
        "WHEEL_RADIUS": 0.3,

        "WHEEL_LEFT_FRONT_POSITION":  [-0.75, -0.45, -1.45],
        "WHEEL_RIGHT_FRONT_POSITION": [ 0.75, -0.45, -1.45],
        "WHEEL_LEFT_REAR_POSITION":   [-0.75, -0.45,  1.15],
        "WHEEL_RIGHT_REAR_POSITION":  [ 0.75, -0.45,  1.15],

        "ANGULAR_VELS_FRONT": [-6000, -4200, -3000, 0, -1],
        "ANGULAR_VELS_REAR": [-6000, -4200, -3000, 0, -1],

        "POWERS_FRONT": [20, 200,  200,  20, 20],
        "POWERS_REAR": [20, 200,  200,  20, 20],

        "GEAR_RATIOS": [0.75, 1.5, 3, 6, 9, -9]
      }
    }
  },
  {
    "declare_macro": "set_spawner_nodes_FN",
    "content": {
      "call": "spawner_set_nodes",
      "literals": {
        "nodes": ["car_node$SUFFIX"],
        "spawner": "%SPAWNER_NAME"
      }
    }
  },
  {
    "declare_macro": "create_player_car_externals_FN",
    "content": {
      "playback": "generic_car.create_player_car_externals",
      "literals": {
        "ASSET_ID": "FN",
        "IF_PC": "%IF_PC",
        "IF_MANUAL_DRIVE": "%IF_MANUAL_DRIVE",
        "PLAYER_NAME": "%PLAYER_NAME",
        "SUFFIX": "%SUFFIX",

        "CAMERA_CLASS": "sedan",
        "COCKPIT_CAMERA_POSITION": [-0.4, 0.4, -0.3]
      }
    }
  }
]