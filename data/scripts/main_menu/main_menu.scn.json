          "bottom": "submenu_bottom",
          "top": "submenu_top",
          "ttf_file": "fonts/RobotoMono-Bold.ttf",
          "font_color": "%ui_font_color",
          "font_height": "normal",
          "line_distance": "normal",
          "deflt": 0,
          "parameters": [
            {"id": "day", "title": "Day", "on_before_select": { "call": "globals", "arguments": {"skybox": "skybox", "time_of_day": "day"}}},
            {"id": "night", "title": "Night", "on_before_select": { "call": "globals", "arguments": {"skybox": "skybox_night", "time_of_day": "night"}}},
            {"id": "synthwave", "title": "Synthwave", "on_before_select": { "call": "globals", "arguments": {"skybox": "skybox_night", "time_of_day": "synthwave"}}}
