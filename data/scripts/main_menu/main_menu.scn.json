          "font_color": "%ui_font_color",
          "font_height": "normal",
          "line_distance": "normal",
          "deflt": 0,
          "z_order": 2
        }
      },
      {
        "call": "tab_menu",
        "arguments": {
          "id": "new_game_submenus",
          "focus_mask": "new_game_menu",
          "selection_marker": "selection_marker",
          "style": "icon",
          "charset": "%loaded_language",
          "ttf_file": "fonts/RobotoMono-Bold.ttf",
          "icon_left": "tabmenu_icon_left",
          "icon_right": "tabmenu_icon_right",
          "icon_bottom": "tabmenu_icon_bottom",
          "icon_top": "tabmenu_icon_top",
          "left": "tabmenu_left",
          "right": "tabmenu_right",
          "bottom": "tabmenu_bottom",
          "top": "tabmenu_top",
          "font_color": "%ui_font_color",
          "font_height": "normal",
          "line_distance": "normal",
          "deflt": 0,
          "on_execute": [
            {
              "required": ["%selected_level_id != %loaded_level_id"],
              "execute": {
                "call": "reload_scene",
                "arguments": {
                  "change_scene": true
                }
              }
            },
            {
              "required": ["%selected_level_id == %loaded_level_id"],
              "execute": {
                "playback": "_reload_transient_objects_in_physics_thread"
              }
            }
          ],
          "z_order": 2
        }
      }
    ]
  },
  {
    "declare_macro": "parameter_setters",
    "content": [
      {
        "call": "add_charset",
        "arguments": {
          "name": "language",
          "chars": "' 0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ<>,;:()[]{}\"รง'"
        }
      },
      {
        "call": "parameter_setter",
        "arguments": {
          "id": "language",
          "charset": "language",
          "focus_mask": "settings_menu",
          "title": "%%translations/main/Language/$loaded_language",
          "icon": "main_icon",
          "left": "submenu_left",
          "right": "submenu_right",
          "bottom": "submenu_bottom",
          "top": "submenu_top",
          "ttf_file": "fonts/RobotoMono-Bold.ttf",
          "font_color": "%ui_font_color",
          "font_height": "normal",
          "line_distance": "normal",
          "deflt": 0,
          "persistent": "language",
          "assets": "languages",
          "on_change": [
            {
              "required": ["%selected_language != %loaded_language"],
              "execute": {
                "call": "set_requires_reload",
                "arguments": {
                  "id": "language",
                  "reason": "Language changed"
                }
              }
            },
            {
              "required": ["%selected_language == %loaded_language"],
              "execute": {
                "call": "clear_requires_reload",
                "arguments": {
                  "id": "language"
                }
              }
            }
          ]
        }
      },
      {
        "call": "reload_required",
        "arguments": {
          "title": "Reload required to apply the following changes:",
          "focus_mask": "main_menu",
          "left": "showroom_left",
          "right": "showroom_right",
          "bottom": "showroom_bottom",
          "top": "showroom_top",
          "ttf_file": "fonts/RobotoMono-Bold.ttf",
          "font_color": "%ui_font_color",
          "font_height": "normal",
          "line_distance": "normal",
          "z_order": 2
        }
      },
      {
        "call": "parameter_setter",
        "arguments": {
          "id": "main_menu",
          "focus_mask": "main_menu",
          "submenus": [],
          "title": "Main menu",
          "icon": "main_icon",
          "required": [],
          "left": "submenu_left",
          "right": "submenu_right",
          "bottom": "submenu_bottom",
          "top": "submenu_top",
          "charset": "%loaded_language",
          "ttf_file": "fonts/RobotoMono-Bold.ttf",
          "font_color": "%ui_font_color",
          "font_height": "normal",
          "line_distance": "normal",
          "deflt": 0,
          "parameters": [
            {
              "id": "new_game",
              "title": "%%translations/main/New_game/$loaded_language",
              "on_execute": { "call": "append_focuses", "arguments": {"content": ["new_game_menu"]}}
            },
            {
              "id": "settings",
              "title": "%%translations/main/Settings/$loaded_language",
              "on_execute": { "call": "append_focuses", "arguments": {"content": ["settings_menu"]}}
            },
            {
              "id": "reload",
              "title": "%%translations/main/Reload/$loaded_language",
              "on_execute": { "call": "reload_scene", "arguments": {"change_scene": false}}
            },
            {
              "id": "exit",
              "title": "%%translations/main/Exit/$loaded_language",
              "on_execute": { "call": "exit" }
            }
          ]
        }
      },
      {
        "#comment": "Some initial color so the code does not crash until the real color is defined",
        "call": "globals",
        "arguments": { "SELECTED_PLAYER_VEHICLE_COLOR": [1, 1, 0] }
      },
      {
        "required": ["%if_devel"],
        "call": "parameter_setter",
        "arguments": {
          "id": "time_of_day_selector",
          "focus_mask": "new_game_menu",
          "title": "Time of day",
          "icon": "time_of_day_icon",
          "required": [],
