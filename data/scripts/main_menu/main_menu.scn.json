          },
          "widget": {
            "left": "tabmenu_left",
            "right": "tabmenu_right",
            "bottom": "tabmenu_bottom",
            "top": "tabmenu_top"
          },
          "font_color": "%ui_font_color",
          "font_height": "normal",
          "line_distance": "normal",
          "user_id": "%user_id",
          "deflt": 0,
          "on_execute": [
            {
              "required": "(%selected_level_id != %loaded_level_id)",
              "execute": {
                "call": "reload_scene",
                "arguments": {
                  "change_scene": true
                }
              }
            },
            {
              "required": "(%selected_level_id == %loaded_level_id)",
              "execute": {
                "playback": "_reload_transient_objects_in_physics_thread",
                "without": ["user_id", "user_name"]
              }
            }
          ],
          "z_order": 2
        }
      }
    ]
